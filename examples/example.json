{
  "logging": {
    "filters": [
      {
        "enabled": "true",
        "tag": "stdout",
        "type": "console",
        "level": "DEBUG",
        "properties": [
          {
            "name": "json",
            "value": "false"
          },
          {
            "name": "format",
            "value": "[%D %T] [%L] (%S) (%P) %M"
          }
        ]
      },
      {
        "enabled": "true",
        "tag": "debug_file",
        "type": "file",
        "level": "DEBUG",
        "properties": [
          {
            "name": "filename",
            "value": "logs/examples_debug.log"
          },
          {
            "name": "json",
            "value": "false"
          },
          {
            "name": "format",
            "value": "[%D %T] [%L] [%S] [%P] %M"
          },
          {
            "name": "rotate",
            "value": "true"
          },
          {
            "name": "maxsize",
            "value": "0M"
          },
          {
            "name": "maxlines",
            "value": "0K"
          },
          {
            "name": "maxbackup",
            "value": "16"
          },
          {
            "name": "daily",
            "value": "false"
          },
          {
            "name": "hourly",
            "value": "1"
